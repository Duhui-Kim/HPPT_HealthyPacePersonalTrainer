package com.diet.model.dto;

public class Food {
	// 번호
	private int NUM;
	// 식품명
	private String DESC_KOR;
	// 총 내용량
	private int SERVING_SIZE;
	// 열량(kcal)
	private int NUTR_CONT1;
	// 탄수화물
	private int NUTR_CONT2;
	// 단백질
	private int NUTR_CONT3;
	// 지방
	private int NUTR_CONT4;

	public Food() {
	}

	public Food(int nUM, String dESC_KOR, int sERVING_SIZE, int nUTR_CONT1, int nUTR_CONT2, int nUTR_CONT3,
			int nUTR_CONT4) {
		super();
		NUM = nUM;
		DESC_KOR = dESC_KOR;
		SERVING_SIZE = sERVING_SIZE;
		NUTR_CONT1 = nUTR_CONT1;
		NUTR_CONT2 = nUTR_CONT2;
		NUTR_CONT3 = nUTR_CONT3;
		NUTR_CONT4 = nUTR_CONT4;
	}

	public int getNUM() {
		return NUM;
	}

	public void setNUM(int nUM) {
		NUM = nUM;
	}

	public String getDESC_KOR() {
		return DESC_KOR;
	}

	public void setDESC_KOR(String dESC_KOR) {
		DESC_KOR = dESC_KOR;
	}

	public int getSERVING_SIZE() {
		return SERVING_SIZE;
	}

	public void setSERVING_SIZE(int sERVING_SIZE) {
		SERVING_SIZE = sERVING_SIZE;
	}

	public int getNUTR_CONT1() {
		return NUTR_CONT1;
	}

	public void setNUTR_CONT1(int nUTR_CONT1) {
		NUTR_CONT1 = nUTR_CONT1;
	}

	public int getNUTR_CONT2() {
		return NUTR_CONT2;
	}

	public void setNUTR_CONT2(int nUTR_CONT2) {
		NUTR_CONT2 = nUTR_CONT2;
	}

	public int getNUTR_CONT3() {
		return NUTR_CONT3;
	}

	public void setNUTR_CONT3(int nUTR_CONT3) {
		NUTR_CONT3 = nUTR_CONT3;
	}

	public int getNUTR_CONT4() {
		return NUTR_CONT4;
	}

	public void setNUTR_CONT4(int nUTR_CONT4) {
		NUTR_CONT4 = nUTR_CONT4;
	}

}
